{
  "name": "memconfig",
  "version": "0.1.2",
  "description": "Manage project-wide configuration from an immutable in-memory store, using a friendly interface powered by lodash's get/set path-like selectors",
  "keywords": [
    "configuration",
    "config",
    "node",
    "browser",
    "manager"
  ],
  "main": "dist/memconfig.umd.js",
  "module": "dist/memconfig.es5.js",
  "types": "dist/types/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "author": "Frondor <frondor.dev@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "lint": "eslint . --ext .js,.ts --fix",
    "prebuild": "rimraf dist",
    "build": "tsc --module commonjs && rollup -c rollup.config.ts",
    "test": "jest --coverage",
    "preversion": "git merge master && git push -u origin HEAD && npm ci && npm run lint && npm run build && npm test",
    "postversion": "git push && git push --tags"
  },
  "devDependencies": {
    "@types/jest": "^25.2.3",
    "@types/lodash": "^4.14.152",
    "@types/node": "^14.0.5",
    "@typescript-eslint/eslint-plugin": "^3.0.0",
    "@typescript-eslint/parser": "^3.0.0",
    "eslint": "^7.1.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^26.0.1",
    "jest-config": "^26.0.1",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.10.9",
    "rollup-plugin-bundle-size": "^1.0.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-sourcemaps": "^0.6.2",
    "rollup-plugin-terser": "^6.1.0",
    "rollup-plugin-typescript2": "^0.27.1",
    "rollup-plugin-uglify": "^6.0.4",
    "ts-jest": "^26.0.0",
    "ts-node": "^8.10.1",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.15.0",
    "tslint-config-standard": "^9.0.0",
    "typescript": "^3.0.3"
  },
  "dependencies": {
    "lodash": "^4.17.15"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Frondor/memconfig.git"
  },
  "bugs": {
    "url": "https://github.com/Frondor/memconfig/issues"
  },
  "homepage": "https://github.com/Frondor/memconfig#readme"
}
